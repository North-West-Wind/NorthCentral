{
	"functions": {
		"api/index.ts": {
			"memory": 256
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=1, stale-while-revalidate=59"
				}
			]
		}
	]
}